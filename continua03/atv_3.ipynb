    "def SomaFracoes(n,x):\n",
    "    y = 1\n",
    "    w = 0\n",
    "    soma = 1;\n",
    "    for i in range(x-1):\n",
    "        prog = n\n",
    "        w = y + prog\n",
    "        y = w\n",
    "        n+= 2\n",
    "        soma += 1/w\n",
    "    print(\"{:.6f}\".format(soma))\n",
    "        \n"
   ]
